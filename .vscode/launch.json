    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Bot",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\start.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Register commands",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\registerCommands.js"
        },
       {
        "type": "pwa-node",
        "request": "launch",
        "name": "Remove commands",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "program": "${workspaceFolder}\\src\\removeCommands.js"
        }, 
       
    ]
}